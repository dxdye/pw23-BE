# PostgreSQL Configuration
DB_NAME=pw23
DB_USER=cache_user
DB_PASSWORD=cache_pass
DB_PORT=5433

# Comma-separated GitHub accounts to cache
GITHUB_ACCOUNTS=dxdye,d2tsb

# Cron schedule for cache refresh (cron format)
# Examples: */5 * * * * (every 5 min), */1 * * * * (every 1 min), 0 * * * * (every hour)
CACHE_REFRESH_CRON=*/1 * * * *

# Optional GitHub token to increase rate limits
# Get it from: https://github.com/settings/tokens
# GITHUB_TOKEN=ghp_your_token_here

# App port
PORT=8000

# Cache table names (usually no need to change these)
# CACHE_TABLE=cache_entries
# VERSIONS_TABLE=cache_versions